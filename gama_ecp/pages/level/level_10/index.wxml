<import src="../../template/win/index.wxml" />
<import src="../../template/fail/index.wxml" />
<import src="../../template/getprop/index.wxml" />
<import src="../../template/loading/index.wxml" />
<import src="../../template/menu/index.wxml" />
<swiper wx:if="{{level10Event==0}}" class='containerL' indicator-dots="{{true}}" bindchange="bindChange" current='{{current}}' indicator-active-color="#fff">
  <swiper-item class="con_item ">
    <image class='level_1_1' src='/assets/sprite/level_1_1.png' />
    <!--同事  -->
    <image class='level_1_12' src='/assets/sprite/level_1_12.png' />
    <!--锤子  -->
    <image class='level_9_1' wx:if="{{sprite.hammer.status==0}}" bindtap='spriteChangeHifi' data-sprite="hammer" src='/assets/sprite/level_10_3.png' />

    <!--裂口 -->
    <image class='level_10_1' wx:if="{{sprite.cleft.status==0}}" src='/assets/sprite/level_10_1.png' />
    <image class='level_10_2' wx:else src='/assets/sprite/level_10_2.png' />
    <!--钥匙  -->
    <image class='level_10_3' wx:if="{{sprite.cleft.status==1&&sprite.key.status==0}}" bindtap='spriteChangeHifi' data-sprite="key" src='/assets/sprite/level_1_5.png' />

  </swiper-item>
  <swiper-item class="con_item">
    <image class='level_1_3' src='/assets/sprite/level_1_3.png' />
    <!--门  -->
    <image class='level_1_7' wx:if="{{sprite.door.status==0}}" src='/assets/sprite/level_1_7.png' />
    <image class='level_1_6' wx:else bindtap='level10Event1' data-index="1" src='/assets/sprite/level_1_6.png' />

  </swiper-item>
</swiper>

<swiper wx:if="{{level10Event==1}}" class='containerL' indicator-dots="{{true}}" indicator-active-color="#fff">
  <swiper-item class="con_item">
    <image class='level_1_3' src='/assets/sprite/level_1_3.png' />
    <!--门  -->
    <image class='level_1_7' wx:if="{{sprite.door1.status==0}}" bindtap='spriteChange' data-sprite="door1" src='/assets/sprite/level_8_2.png' />
    <image class='level_1_6' wx:else bindtap='level10Event1' data-index="2" src='/assets/sprite/level_10_5.png' />

  </swiper-item>
</swiper>
<swiper wx:if="{{level10Event==2}}" class='containerL' indicator-dots="{{true}}" indicator-active-color="#fff">
  <swiper-item class="con_item">
    <image class='level_1_3' src='/assets/sprite/level_1_3.png' />
    <!--门  -->
    <image class='level_1_7' wx:if="{{sprite.door2.status==0}}" bindtap='spriteChange' data-sprite="door2" src='/assets/sprite/level_8_2.png' />
    <image class='level_1_6' wx:else bindtap='level10Event1' data-index="3" src='/assets/sprite/level_10_5.png' />

  </swiper-item>
</swiper>
<swiper wx:if="{{level10Event==3}}" class='containerL' indicator-dots="{{true}}" indicator-active-color="#fff">
  <swiper-item class="con_item">
    <image class='level_1_3' src='/assets/sprite/level_1_3.png' />
    <!--门  -->
    <image class='level_1_7' wx:if="{{sprite.door3.status==0}}" bindtap='spriteChange' data-sprite="door3" src='/assets/sprite/level_8_2.png' />
    <image class='level_1_6' wx:else bindtap='level10Event1' data-index="4" src='/assets/sprite/level_10_5.png' />

  </swiper-item>
</swiper>
<swiper wx:if="{{level10Event==4}}" class='containerL' indicator-dots="{{true}}" indicator-active-color="#fff">
  <swiper-item class="con_item">
    <image class='level_1_3' src='/assets/sprite/level_1_3.png' />
    <!--门  -->
    <image class='level_1_7' wx:if="{{sprite.door4Left.status==0}}" bindtap='spriteChange' data-sprite="door4Left" src='/assets/sprite/level_8_2.png' />
    <image class='level_1_6' wx:else bindtap='level10Event1' data-index="5" src='/assets/sprite/level_10_5.png' />

  </swiper-item>
  <swiper-item class="con_item">
    <image class='level_1_3' src='/assets/sprite/level_1_3.png' />
    <!--门  -->
    <image class='level_1_7' wx:if="{{sprite.door4Right.status==0}}" bindtap='spriteChange' data-sprite="door4Right" src='/assets/sprite/level_8_2.png' />
    <image class='level_1_6' wx:else src='/assets/sprite/level_10_5.png' bindtap='gameWin' data-index="1" />

  </swiper-item>
</swiper>
<swiper wx:if="{{level10Event==5}}" class='containerL' indicator-dots="{{true}}" indicator-active-color="#fff">
  <swiper-item class="con_item">
    <image class='level_1_3' src='/assets/sprite/level_1_3.png' />

    <image class='level_10_4' src='/assets/sprite/level_10_4.png' />
    <image class='level_3_3' src='/assets/sprite/level_3_3.png' />
  </swiper-item>
</swiper>




<view class='bottom '>
  <image class='bottom_bg ' src='/assets/sprite/level_1_2.png ' />
  <!-- 道具栏 -->
  <view class='prop_con ' wx:for="{{prop}}" wx:key="{{index}}" wx:if="{{item.status==1}}">
    <image class='prop ' src="{{item.url}}" style="left:{{item.x}}rpx;top:{{item.y}}rpx;" data-prop="{{index}}" bindtouchmove="moveEvent" bindtouchend="moveOver" />
  </view>

</view>
<image class='menu ' bindtap='meneChange' src='/assets/texture/menu.png ' />

<view class='mask_layer {{level10EventMove==1?"changeBack1":level10EventMove==2?"changeBack2":level10EventMove==3?"changeBack3":level10EventMove==4?"changeBack4":level10EventMove==5?"changeBack5":""}}' wx:if="{{layerState}}"></view>
<!--胜利亦失败-->
<template is="failPage" data="{{gameState,gameFailImg}}"></template>
<template is="winPage" data="{{gameState,gameSuccessImg}}"></template>
<!--获取道具  -->
<template is="getProp" data="{{getProp}}"></template>
<!--拟加载 -->
<template is="loading" data="{{loadingMap}}"></template>
<!--菜单栏 -->
<template is="menu" data="{{menuShow}}"></template>