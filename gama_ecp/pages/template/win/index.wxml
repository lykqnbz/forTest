<template name="winPage">
  <view class='shadow_layer' wx:if="{{gameState==1}}">
    <image class='win' src='/assets/texture/win.png' />

    <image class='winItem' wx:if="{{gameSuccessImg==1}}" src='/assets/texture/win_1.png' />
    <!-- <image class='winItem' wx:if="{{gameSuccessImg==2}}" src='/assets/texture/win_2.png' /> -->

    <view class='chooseList'>
      <image class='replay' bindtap='replay' src='/assets/texture/replay.png' />
      <image class='otherPass' bindtap='playOther' src='/assets/texture/otherPass.png' />
    </view>
  </view>
</template>