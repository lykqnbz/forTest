<template name="failPage">
  <view class='shadow_layer' wx:if="{{gameState==2}}">
    <image class='fail' src='/assets/texture/fail.png' />
    <button class='share' open-type='share'></button>
    <view class='chooseList'>
      <image class='replay' bindtap='replay' src='/assets/texture/replay.png' />
      <image class='otherPass' bindtap='playOther' src='/assets/texture/otherPass.png' />
    </view>
  </view>
</template>